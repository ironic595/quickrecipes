function toggleMenu(e){const t=document.getElementById('dropdownMenu'),n=document.querySelector('.nav-container');t.style.top=`74px`,'block'===t.style.display?t.style.display='none':t.style.display='block';const o=e=>{n.contains(e.target)||'block'!==t.style.display||(t.style.display='none',document.removeEventListener('click',o))};document.addEventListener('click',o)}
</script>
